    "@testing-library/svelte": "^4.0.5",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.8.3",
    "@types/web-bluetooth": "^0.0.18",
    "@vitest/coverage-istanbul": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "jsdom": "^23.0.1",
    "rimraf": "^5.0.5",
    "rollup": "^3.15.0",
